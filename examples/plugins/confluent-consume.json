                      }
                    ],
                    "required": true,
                    "type": "record"
                  }
                },
                {
                  "ssl_verify": {
                    "default": true,
                    "description": "Set to false to disable SSL certificate verification when connecting to the schema registry.",
                    "type": "boolean"
                  }
                },
                {
                  "ttl": {
                    "between": [
                      0,
                      3600
